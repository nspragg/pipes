{
  "name": "pipes",
  "version": "1.0.0",
  "description": "Treat Node streams like Unix style pipelines",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./src",
    "coverage": "node_modules/.bin/babel-node node_modules/.bin/babel-istanbul cover node_modules/.bin/_mocha -- -R dot",
    "compile": "babel --presets es2015,stage-0 -d lib/ src/",
    "prepublish": "npm run compile",
    "test": "npm run compile && mocha --compilers js:babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nspragg/pipes.git"
  },
  "keywords": [
    "pipes",
    "unix",
    "linux",
    "commands",
    "pipeline",
    "stream"
  ],
  "author": "nspragg@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nspragg/pipes/issues"
  },
  "homepage": "https://github.com/nspragg/pipes",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-istanbul": "^0.11.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "coveralls": "^2.11.9",
    "eslint": "^3.0.0",
    "istanbul": "^0.4.4",
    "lodash": "^4.13.1",
    "mocha": "^2.3.0",
    "sinon": "^1.17.4"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "mocha": true,
      "es6": true
    },
    "rules": {
      "quotes": [
        2,
        "single"
      ],
      "eqeqeq": 2,
      "no-var": 2,
      "one-var": [
        2,
        "never"
      ],
      "prefer-const": 2,
      "semi": 2,
      "strict": [
        2,
        "global"
      ],
      "arrow-parens": 2,
      "prefer-arrow-callback": 2
    }
  },
  "dependencies": {
    "async": "^2.0.1",
    "byline": "^4.2.1",
    "flashheart": "^2.3.0",
    "moment": "^2.14.1"
  }
}
